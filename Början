//Uppgift 1//


let myjewelery = 502;

if ( myjewelery < 499) {
    console.log("You got Iron!");
} else if (myjewelery < 999){
    console.log("You got Silver!");
} else {
    console.log("You got Gold! or something really shitty");
}

