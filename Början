// Uppgift 3 //


for (let i = 1; i <= 13; i++){
    let myrandomInteger = Math.floor(Math.random()*3)
    if (myrandomInteger == 0){
        console.log(`rad ${i}. har 1`)
    } else if (myrandomInteger == 1){
        console.log(`rad ${i}. har x`)
    } else {
        console.log(`rad ${i}. har 2`)

    }

}
